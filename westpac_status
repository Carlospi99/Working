#!/bin/bash# Script to find error on Westpac EUIDecho "please insert EUID:"read EUID1echo "**************************************************************************************"grep $EUID1 /var/lib/tsm/westpac-prod/tmpQuerySD/SUBSCRIBERS_2016* | sed 's/^.....................................//'| sed 's/..........................................................................................................................$//'echo "**************************************************************************************"echo "**************************************************************************************"echo "Insert the latest querySD file name. SUBSCRIBERS_2016xxxx_xxxxxx.txt"read SUBSFILEgrep $EUID1 /var/lib/tsm/westpac-prod/tmpScriptsToCps/out_$SUBSFILEecho "**************************************************************************************"echo " ** Noted:Invalid respond data negative or KO means negative respond from SKMS******"echo " CPLC"grep  $EUID1 /var/lib/tsm/westpac-prod/tmpQuerySD/SUBSCRIBERS_2016* | sed 's/^........................................................................................................//'| sed 's/..$//'echo " CPLC to look in debug"CPCL2=`grep  $EUID1 /var/lib/tsm/westpac-prod/tmpQuerySD/SUBSCRIBERS_2016* | sed 's/^........................................................................................................//'| sed 's/..................................................$//'`echo $CPCL2echo "copy the short CPLC"read CPLC3echo "looking on debug logs"zgrep -B 20 $CPLC3 /var/container_data/westpac-prod/log/debug* | grep IDecho "Copy path of the file"read PATH1echo "**********"echo "Copy ID number"read IDN1zgrep -A 27 "ID: $IDN1" $PATH1
